config udhcp
	option Interface	eth1
	option Start		192.168.1.20
	option end		192.168.1.254
	option route		192.168.1.1
	option subnet		255.255.255.0
	option lease		864000		# default: 10 days

config	dns
	option host		168.126.63.1	# appended to above DNS servers for a total of 1

config	dns
	option host		168.126.63.2	# appended to above DNS servers for a total of 2

